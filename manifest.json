{
  "name": "Yarn for NPMJS",
  "version": "1.0",
  "manifest_version": 3,
  "author": "Chethiya Kusal",
  "description": "Developed by Kusal",
  "homepage_url": "https://github.com/ChethiyaKD",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.npmjs.com/*"],
      "js": ["js/content_script.js"]
    }
  ],
  "background": {
    "service_worker": "js/service_worker.js"
  }
}
